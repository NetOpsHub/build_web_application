server {
        listen 8080 default_server;
        listen [::]:8080 default_server;
        root /var/www/html;
        index index.html;
        server_name _;
        location /test.html {
			default_type "text/html";
			alias /var/www/html/index.html;
        }
        location / {
			rewrite ^/(.*) https://www.google.com/ permanent;
        }
}
